%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
For the following compounds, indicate the expression of $K_{sp}$ in terms of solubility:
\begin{inparaenum}[(a)]
\item  \ce{BaCO3} %Calcium carbonate %$K_{sp}=1\cdot s^2$ %1:1
\item  Calcium fluoride	 %$K_{sp}=4\cdot s^3$ %1:2
\item  \ce{Fe(OH)3} %Iron(III) hydroxide %$K_{sp}=27\cdot s^3$ %1:3
\item  Cobalt(II) arsenate %$K_{sp}=108\cdot s^5$ %2:3
\item  \ce{CdS}  %Cadmium(II) sulfide	%$K_{sp}=1\cdot s^2$ %1:1
 \end{inparaenum}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
\item   $K_{sp}=1\cdot s^2$ %1:1
\item   $K_{sp}=4\cdot s^3$ %1:2
\item   $K_{sp}=27\cdot s^3$ %1:3
\item   $K_{sp}=108\cdot s^5$ %2:3
\item   $K_{sp}=1\cdot s^2$ %1:1
 \end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%%


%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
For the following compounds, indicate the expression of $K_{sp}$ in terms of solubility:
\begin{inparaenum}[(a)]
\item  \ce{MnCO3} %manganese(II) carbonate %$K_{sp}=1\cdot s^2$ %1:1
\item  Scandium(III) hydroxide %$K_{sp}=27\cdot s^3$ %1:3
\item  Copper(I) oxide	%$K_{sp}=4\cdot s^3$ %1:2
\item  \ce{PbF2} %Lead(II) fluoride %$K_{sp}=4\cdot s^3$ %1:2
\item  Iron(II) fluoride	 %$K_{sp}=4\cdot s^3$ %1:2
 \end{inparaenum}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
\item   $K_{sp}=1\cdot s^2$ %1:1
\item   $K_{sp}=27\cdot s^3$ %1:3
\item   $K_{sp}=4\cdot s^3$ %1:2
\item   $K_{sp}=4\cdot s^3$ %1:2
\item   $K_{sp}=4\cdot s^3$ %1:2
 \end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%%
%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
For the following compounds, indicate the expression of $K_{sp}$ in terms of solubility:
\begin{inparaenum}[(a)]
\item  \ce{Cd3(PO4)2} %Cadmium phosphate %$K_{sp}=108\cdot s^5$ %2:3
\item  Mercury(II) sulfide	%$K_{sp}=1\cdot s^2$ %1:1
\item  Calcium phosphate %$K_{sp}=108\cdot s^5$ %2:3
 \end{inparaenum}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
\item  $K_{sp}=108\cdot s^5$ %2:3
\item   $K_{sp}=1\cdot s^2$ %1:1
\item   $K_{sp}=108\cdot s^5$ %2:3
 \end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%%

