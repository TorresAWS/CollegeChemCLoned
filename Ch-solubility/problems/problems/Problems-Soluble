%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
The solubility of \ce{AlNO3} is 82 g/100mL at 30$^{\circ}$C expressed with respect to the solvent. Calculate the grams of solute that can be dissolved in 5mL of water.
\end{question}
\begin{solution}
4.1g
\hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%%




%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
The solubility of ammonium chlorate \ce{NH4ClO4} is 96 g/100mL at 50$^{\circ}$C expressed with respect to the solvent. Calculate the liters of water needed to dissolve 1 gram of ammonium chlorate.
\end{question}
\begin{solution}
$1\times 10^3$L
\hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%%






%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
The solubility of an insoluble solute is 25 g/L at 25$^{\circ}$C expressed with respect to the solution. Calculate the milliliters of solution needed to dissolve 5 grams of solute.
\end{question}
\begin{solution}
$200$mL
\hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%%

%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
The solubility of an insoluble solute is 100 g/L at 20$^{\circ}$C expressed with respect to the solution. Calculate the milliliters of solution needed to dissolve 10 grams of solute.
\end{question}
\begin{solution}
$100$mL
\hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%%

%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
The solubility of an insoluble solute is 4mol/L at a given temperature expressed with respect to the solution. Calculate the moles of solute dissolved in 100mL of solution.
\end{question}
\begin{solution}
0.4mol
\hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%%

%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
The molar solubility of silver carbonate is $1.3\times 10^{-4}$mol/L at 20$^{\circ}$C. Calculate the moles of solute dissolved in 25mL of solution.
\end{question}
\begin{solution}
$3.2\times 10^{-6}$mol
\hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%%


%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
The molar solubility of zinc hydroxide (MW=99.4g/mol) is $4.6\times 10^{-6}$mol/L at 20$^{\circ}$C. Express this value in grams per liter.
\end{question}
\begin{solution}
$4.6\times 10^{-4}$g/L
\hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%%

%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
The molar solubility of lead(II) fluoride (MW=254.2g/mol) is 0.56g/L at 20$^{\circ}$C. Express this value in moles per liter.
\end{question}
\begin{solution}
$2.2\times 10^{-3}$mol/L
\hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%%




