\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Indicate if the following compounds are stable with respect to its corresponding elements at standard conditions:
\begin{inparaenum}[(a)]
\item \ce{SbH3(g)} % no ($G^{\circ}_f>0$)
\item \ce{AlF3(s)} % yes ($G^{\circ}_f<0$)
\item \ce{B2H6(g)} % no ($G^{\circ}_f>0$)
 \end{inparaenum}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
\item   no ($G^{\circ}_f>0$)
\item   yes ($G^{\circ}_f<0$)
\item   no ($G^{\circ}_f>0$)
\end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%% 


\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Indicate if the following compounds are stable with respect to its corresponding elements at standard conditions:
\begin{inparaenum}[(a)]
\item \ce{As2O5(s)} % yes ($G^{\circ}_f<0$)
\item \ce{AsH3(g)} % no ($G^{\circ}_f>0$)
\item \ce{BaCl2(s)} % yes ($G^{\circ}_f<0$)
 \end{inparaenum}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
 \item   yes ($G^{\circ}_f<0$)
\item   no ($G^{\circ}_f>0$)
\item   yes ($G^{\circ}_f<0$)
\end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%% 

\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Compute the standard Gibbs free energy for the following reaction:\\
\begin{adjustbox}{width=\columnwidth,center}
 \fontfamily{ppl}\selectfont
\begin{tabular}{llllllll}
\toprule
 &					\ce{4NH3_{(g)}}&\ce{+}	& \ce{5O2_{(g)}}    &\ce{->}	 &\ce{4NO_{(g)}}     &\ce{+} &\ce{6H2O_{(g)}}        \\
\midrule
 $G^{\circ}_f(KJ/mol)$&	-16.45	&	&  0  & &	86.55    & & -228.57       \\
\midrule
 $n$&	&	&     & &     & &        \\
\midrule
 $G^{\circ}_f\cdot n$(KJ)&	&	&     & &     & &        \\
  \midrule
  $\Delta G^{\circ}_f(KJ) $ & \multicolumn{7}{c}{  	}     \\
\bottomrule
\end{tabular} 
\end{adjustbox} 
\end{question}
\begin{solution}
 $-959.4 $KJ
\hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%% 



