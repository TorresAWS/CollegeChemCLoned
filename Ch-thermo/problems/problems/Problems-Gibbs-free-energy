\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Compute the standard Gibbs free energy for the following reaction at 298K:\\
\begin{adjustbox}{width=\columnwidth,center}
 \fontfamily{ppl}\selectfont
\begin{tabular}{llllllll}
\toprule
 &					\ce{CO_{(g)}}&\ce{+}	& \ce{H2O_{(g)}}    &\ce{->}	 &\ce{CO2_{(g)}}     &\ce{+} &\ce{H2_{(g)}}        \\
\midrule
 $H^{\circ}_f(KJ/mol)$&	-110.525	&	&  -241.818  & &	-393.509    & & 0      \\
\midrule
 $S^{\circ}( J/molK)$&		197.674	&	&  188.825  & &	213.74    & & 130.684      \\
\midrule
 $G^{\circ}_f\cdot n$(KJ)&	&	&     & &     & &        \\
  \midrule
  $\Delta G^{\circ}_f(KJ) $ & \multicolumn{7}{c}{  	}     \\
\bottomrule
\end{tabular} 
\end{adjustbox} 
\end{question}
\begin{solution}
 $-28.62$KJ
\hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%% 




\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
