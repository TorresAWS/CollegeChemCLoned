\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
A hot container submerged in water releases 100J of energy at 298K. Calculate the entropy change of the hot container.
\end{question}
\begin{solution}
 -0.33J/K
\hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%%

\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
An cup of milk cools down in a refrigerator at 15$^{\circ}$C, releasing 20J. Calculate the change in entropy in the milk.
\end{question}
\begin{solution}
 -0.07J/K
\hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%%

