
%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Answer the following questions involving dosages:    
  \begin{inparaenum}[(a)]
\item An IV solution needs to be delivered at a rate of 120mL/h. How long it takes to deliver 200mL? %1.7h
\item Infant Tylenol is given from a 160mg/5mL suspension based on the infant's weight. A 12lb infant requires 2.5mL. How many milligrams of Tylenol are contained in the dosage given? %80mg 
\end{inparaenum}
\end{question}
\begin{solution}
  \begin{inparaenum}[(a)]
 \item  1.7h
\item  80mg
\end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%
 


%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Answer the following questions involving dosages:    
  \begin{inparaenum}[(a)]
\item A patient requires 1g of medication given every three hours. The medication in stock was found in tablets of 200mg. How many tablets do you need in three days?	% 120 tablets
\item A medication needs to be given based on the patient's body weight as 2mg/Kg. If a patient weighs 70Kg and the mediation stock is 100mg/mL, how many mL are needed? %1.4mL  
\end{inparaenum}
\end{question}
\begin{solution}
  \begin{inparaenum}[(a)]
\item   120 tablets
\item  1.4mL  
 \end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%
