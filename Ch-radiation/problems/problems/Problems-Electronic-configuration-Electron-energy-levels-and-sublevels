%%%%%%%PROBLEM
\vspace{-0.0cm}\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Match the following statements with:
 \begin{inparaenum}[(1)]
\item     have the same shape
\item     can fit the same number of electrons
\item     belong to the same energy level
\item     can fit the same number of electrons
\end{inparaenum}
 \begin{inparaenum}[(a)]
\item    1s and 2s orbitals 	%have the same shape and can fit the same number of electrons
\item    five 3d orbitals 	%can fit the same number of electrons
\item    2p and 3p sublevels 	%can fit the same number of electrons
\item    3s and 3p sublevels 	%belong to the same energy level
\end{inparaenum}
 \end{question}
\begin{solution}
 \begin{inparaenum}[(a)]
\item     have the same shape and can fit the same number of electrons
\item     can fit the same number of electrons
\item     can fit the same number of electrons
\item     belong to the same energy level 
\end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}
%%%%%%%%%%%%%%
%%%%%%%PROBLEM
\vspace{-0.0cm}\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Match the following statements with:
 \begin{inparaenum}[(1)]
\item     have the same shape
\item     can fit the same number of electrons
\item     belong to the same energy level
\item     can fit the same number of electrons
\end{inparaenum}
 \begin{inparaenum}[(a)]
\item    4p and 3p orbitals 	%have the same shape and can fit the same number of electrons
\item    three 3p orbitals 	%can fit the same number of electrons
\item    3d and 3s sublevels 	%belong to the same energy level
\item    3p and 5p sublevels 	%can fit the same number of electrons
\end{inparaenum}
 \end{question}
\begin{solution}
 \begin{inparaenum}[(a)]
\item     have the same shape and can fit the same number of electrons
\item     can fit the same number of electrons
\item     belong to the same energy level
\item     can fit the same number of electrons
\end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}
