%%%%%%%PROBLEM
\vspace{-0.0cm}\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Describe the shape of the following orbitals using the terms lobular, spherical, or lobular(dumbbell shape):
 \begin{inparaenum}[(a)]
\item    3d 	%lobular 
\item    1s   %spherical
\item    2p 	%lobular(dumbbell shape)
\item    2s   %spherical
\end{inparaenum}
 \end{question}
\begin{solution}
 \begin{inparaenum}[(a)]
\item     lobular 
\item     spherical
\item     lobular(dumbbell shape)
\item     spherical 
\end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}
%%%%%%%%%%%%%%
%%%%%%%PROBLEM
\vspace{-0.0cm}\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Describe the shape of the following orbitals using the terms lobular, spherical, or lobular(dumbbell shape):
 \begin{inparaenum}[(a)]
\item    4p 	%lobular(dumbbell shape)
\item    4d 	%lobular 
\item    3p 	%lobular(dumbbell shape)
\item    3s   %spherical
\end{inparaenum}
 \end{question}
\begin{solution}
 \begin{inparaenum}[(a)]
\item     lobular(dumbbell shape)
\item     lobular 
\item     lobular(dumbbell shape)
\item     spherical  
\end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}
