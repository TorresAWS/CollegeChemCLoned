   %%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Identify the functional groups in the following molecule:
\begin{center}
\chemfig{-[::+45]=[::-45]-[::+45]OH}
\end{center}
\end{question}\begin{solution}
alkene and alcohol
 \end{solution}\stepcounter{numA}
