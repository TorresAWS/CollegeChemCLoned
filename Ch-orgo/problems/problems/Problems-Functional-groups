%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Identify the functional groups in the following molecule:\begin{center}
\chemfig{*6((-H_3C)---N(-C(=[::+60]O)-[::-60])---)}
\end{center}
\end{question}\begin{solution}
amide
 \end{solution}\stepcounter{numA}
%%%%%%%%%%%%%%
 
 
  %%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Identify the functional groups in the following molecule and give the molecular formula:\begin{center}
\chemfig{**6(---(-NH _2)---)}
\end{center}
\end{question}\begin{solution}
amine and aromatic; \ce{C6H7N}
 \end{solution}\stepcounter{numA}
%%%%%%%%%%%%%%
 
  %%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Identify the functional groups in the following molecule and give the molecular formula:\begin{center}
\chemfig{**6(--(-OH)----)}
\end{center}
\end{question}\begin{solution}
alcohol and aromatic;  \ce{C6H6O}
 \end{solution}\stepcounter{numA}
