  %%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Name or formulate the following chemicals:
\begin{center}\begin{inparaenum}[(a)]
        \item   \chemfig{ -[:45]((-[:90]-[:45])(-[:-45]((-[:-45]I)(-[:-135]I)(-[:45]-[:-45](((-[:-90]Br))(-[:45]~[:45]-[:45]))))))  }   
        \item   \chemfig{ -[:45]( (-[:-45]((-[:-90]F)(-[:45]-[:-45](((-[:-90]))(-[:45]~[:45]-[:45]))))))  }   
\end{inparaenum}\end{center}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
        \item    4-bromo-5,5-diiodo-2-nonyne         
        \item     4-methyl-5-fluoro-2-octyne    
\end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}
