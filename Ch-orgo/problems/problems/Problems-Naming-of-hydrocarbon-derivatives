   \begin{center}
      \chemfig{ CH_3-C(-[:90]CH_3)(-[:-90]CH_3)(-CH_2-CH_2-CH_2OH)}     % 4,4-dimethyl-1-pentanol, primary
   \end{center}
 \end{question}
\begin{solution}
