  %%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Name or formulate the following alcohol and label it as primary, secondary or tertiaty:
   \begin{center}
   \chemfig{ CH_3-CH_2-CH_2-CH((-[:90]OH)(-CH_2-CH((-[:-90]CH_2-CH_3)(-CH_3)))) }     % 6-methyl-4-octanol, secundary
   \end{center}
 \end{question}
\begin{solution}
     6-methyl-4-octanol, secundary
\hspace{0.1cm}\end{solution}\stepcounter{numA}
%%%%%%%%%%%%%%

  %%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
