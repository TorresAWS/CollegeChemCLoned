 %%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Write down the expanded formula for hexane.
\begin{center}
\setpolymerdelim()
\chemfig{-[:45]-[@{A,0.5}:-45,1]-[:45]-[@{B,0.5}:-45,1] -[:45]}\makebraces(12pt,12pt){$\scriptstyle\!\!3$}{A}{B}
\end{center}
\end{question}
\begin{solution}
\small \setpolymerdelim()\chemfig{-[:45]-[@{A,0.5}:-45,1]-[:45]-[@{B,0.5}:-45,1] -[:45]}\makebraces(9pt,9pt){$\scriptstyle\!\!1$}{A}{B}\end{solution}\stepcounter{numA}

