%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Identify the group number described by:
\begin{inparaenum}[(a)]
\item Starts with Zn %2B
\item Ends with Ra %2A
\item Contains the elements  O, S, and Se %6A
\item Is located to the right of Mn %8
\item Is located to the left of Ti %3B
\end{inparaenum}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
\item  2B
\item  2A
\item  6A
\item  8
\item  3B
\end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}
%%%%%%%%%%%%%%
%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Identify the group number described by:
\begin{inparaenum}[(a)]
\item Starts with B % 3A
\item Ends with Rn %8A
\item Contains the elements  Mn, Tc, and Re %7B
\item Is located to the right of H %2A
\item Is located to the left of F %6A
\end{inparaenum}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
\item   3A
\item  8A
\item  7B
\item  2A
\item  6A
\end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}
%%%%%%%%%%%%%%



%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Identify the elements located at:
\begin{inparaenum}[(a)] 
\item group 7A and period 3  %  Ar
 \item An halogen at period  5 %I  
 \item An alkaline earth at period 7  %  Ra
 \item An alkali  at period 3 %Na 
\end{inparaenum}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
\item    Ar
 \item  I  
 \item    Ra
 \item  Na 
\end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}
%%%%%%%%%%%%%%
%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Identify the elements located at:
\begin{inparaenum}[(a)]
\item group 2A and period 4  %  Ca
 \item An halogen at period  3 %Cl  
 \item An alkaline earth at period 5  %  Sr
 \item An alkali  at period 6 %Cs  
\end{inparaenum}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
 \item    Ca
 \item   Cl  
 \item    Sr
 \item  Cs  
\end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}








%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Identify the elements below as metal, nonmetal or metalloid:
\begin{inparaenum}[(a)]
\item  B % metalloid  
\item  Sb % metalloid  
\item  Fe % metal  
\item  Zn % metal  
\item  H % nonmetal  
\end{inparaenum}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
\item    metalloid  
\item    metalloid  
\item    metal  
\item    metal  
\item   nonmetal   
\end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}
%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Identify the elements below as metal, nonmetal or metalloid:
\begin{inparaenum}[(a)]
\item  A shinny element % metal
\item  C % nonmetal    
\item  A electrically conducting element % metal  
\item  A dull element % nonmetal  
\item  A semiconductor % metalloid   
\end{inparaenum}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
 \item    metal
\item   nonmetal    
\item    metal  
\item    nonmetal  
\item    metalloid    
\end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}
