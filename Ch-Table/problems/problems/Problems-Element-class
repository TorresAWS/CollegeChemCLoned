\begin{multicols}{2}
  \noindent
  \begin{enumerate} [topsep=0pt, partopsep=1pt, label=(\alph*), leftmargin=1cm]
\item Be
\item Mg
\item Ca
\item C
\item Si
\end{enumerate}
\end{multicols}    
\end{question}
\begin{solution}
(c)
\hspace{0.1cm}\end{solution}\stepcounter{numA}
