%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Consider a neutral atom with 30 protons and 34 neutrons. The atomic number of the element is:
\begin{inparaenum}[(a)]
\item 30
\item 32
\item 34
\item 64
\item 94
\end{inparaenum}
 \end{question}
\begin{solution}
(a)
\hspace{0.1cm}\end{solution}\stepcounter{numA}
%%%%%%%%%%%%%%




%%%%%%%PROBLEM
