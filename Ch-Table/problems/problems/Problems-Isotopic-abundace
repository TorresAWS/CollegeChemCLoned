

%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Calculate the number of protons, neutrons and electrons in the following isotopes:
\begin{inparaenum}[(a)]
\item \ce{^{15}_{7}N}   % 7 protons, 7 electrons and 8 neutrons
\item \ce{^{64}_{29}Cu}   % 29 protons, 29 electrons and 35 neutrons
\item \ce{^{93}_{40}Zr}   % 40 protons, 40 electrons and 53 neutrons
 \end{inparaenum}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
\item   7 protons, 7 electrons and 8 neutrons
\item   29 protons, 29 electrons and 35 neutrons
\item   40 protons, 40 electrons and 53 neutrons 
\end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}
%%%%%%%%%%%%%%


%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Calculate the number of protons, neutrons and electrons in the following isotopes:
\begin{inparaenum}[(a)]
\item \ce{^{79}_{34}Se}   % 34 protons, 34 electrons and 45 neutrons
\item \ce{^{85}_{36}Kr}   % 36 protons, 36 electrons and 49 neutrons
\item \ce{^{13}_{7}N}   % 7 protons, 7 electrons and 6 neutrons
 \end{inparaenum}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
\item   34 protons, 34 electrons and 45 neutrons
\item   36 protons, 36 electrons and 49 neutrons
\item   7 protons, 7 electrons and 6 neutrons 
\end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}
%%%%%%%%%%%%%%




%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Write down the isotopic symbol for the atom or ion represented below indicating the name of the element.
\definecolor{mylightred}{RGB}{255,210,210}
\definecolor{myred}{RGB}{200,100,100}
\definecolor{mydarkred}{RGB}{140,40,40}
\definecolor{mylightblue}{RGB}{220,228,255}
\definecolor{myblue}{RGB}{183,191,229}
\definecolor{mydarkblue}{RGB}{50,70,190}
\begin{center}\begin{tikzpicture}[scale=1]
\tikzset{
    pics/proton/.style={code={\shade[ball color=red] circle (3pt);}},
    pics/neutron/.style={code={\shade[ball color=white] circle (3pt);}},
    pics/nucleussmall/.style={code={%
        \pgfmathdeclarerandomlist{nucleon}{{proton}{proton}{neutron}{neutron}{neutron}}
        \pgfmathsetseed{#1+1}
        \foreach \A/\R in {8/0.2, 5/0.13, 1/0}{
        \pgfmathsetmacro{\S}{360/\A}
            \foreach \B in {0,\S,...,360}{
                \pgfmathrandomitem{\C}{nucleon}
                \pic at ($(\B+2*\A+5*rnd:\R)$) {\C}; } }} },
    pics/nucleusbig/.style={code={%
        \pgfmathdeclarerandomlist{nucleon}{{proton}{proton}{neutron}{neutron}{neutron}}
        \pgfmathsetseed{#1+1}
        \foreach \A/\R in {24/0.4, 24/0.3, 24/0.2, 13/0.35, 11/0.27, 6/0.15, 1/0}{
        \pgfmathsetmacro{\S}{360/\A}
            \foreach \B in {0,\S,...,360}{
                \pgfmathrandomitem{\C}{nucleon}
                \pic at ($(\B+2*\A+5*rnd:\R)$) {\C}; } }} },
    pics/nucleusbiggest/.style={code={%
        \pgfmathdeclarerandomlist{nucleon}{{proton}{proton}{neutron}{neutron}{neutron}}
        \pgfmathsetseed{#1+1}
        \foreach \A/\R in {24/0.5, 24/0.4, 24/0.3, 24/0.2, 13/0.47, 15/0.44, 13/0.37, 11/0.27, 6/0.15, 1/0}{
        \pgfmathsetmacro{\S}{360/\A}
            \foreach \B in {0,\S,...,360}{
                \pgfmathrandomitem{\C}{nucleon}
                \pic at ($(\B+2*\A+5*rnd:\R)$) {\C}; } }} },
    }
 \tikzset{square left brace/.style={ncbar=0.5cm}}
\tikzset{square right brace/.style={ncbar=-0.5cm}}
 \begin{scope}[shift={(0,0)},rotate=00] 
\filldraw[even odd rule,inner color=mydarkred,outer color=white, line width=0, draw=white] (0,0) circle (2.0);
  \coordinate (O)  at (0,0);
   \fill[radius=2.0pt,mydarkblue] (O) circle ;
\pic at (0,0) {nucleussmall} node at  (0,-1) { } ;

\draw[ -, >=latex'](0.3,0.2) -- coordinate (x axis) (0.9,0.9); 
\draw[ -, >=latex'](0.9,-0.9) -- coordinate (x axis) (1.7,-1.7);       
      
\draw[decorate,decoration={brace,amplitude=3pt,mirror}] (1.1,1.5) coordinate (t_k_unten) -- (1.1,0.5) coordinate (t_k_opt_unten); 
\node at (1.5,1.2){$n_p$=7};
\node at (1.5,0.7){$n_n$=7};
\node at (2.2,-1.7){$n_e$=10};
\draw [red, thick] (-2,-2) to [square left brace ] (-2,2);
\draw [red, thick] (2.2,-2) to [square right brace] (2.2,2);
\node [red] at (3,2){\Large $3-$};
  \end{scope}
\end{tikzpicture}\end{center}
 \end{question}
\begin{solution}
 \ce{^{14}_{7}N^{3-}}
\hspace{0.1cm}\end{solution}\stepcounter{numA}
%%%%%%%%%%%%%%


%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Write down the isotopic symbol for the atom or ion represented below indicating the name of the element.
\definecolor{mylightred}{RGB}{255,210,210}
\definecolor{myred}{RGB}{200,100,100}
\definecolor{mydarkred}{RGB}{140,40,40}
\definecolor{mylightblue}{RGB}{220,228,255}
\definecolor{myblue}{RGB}{183,191,229}
\definecolor{mydarkblue}{RGB}{50,70,190}
\begin{center}\begin{tikzpicture}[scale=1]
\tikzset{
    pics/proton/.style={code={\shade[ball color=red] circle (3pt);}},
    pics/neutron/.style={code={\shade[ball color=white] circle (3pt);}},
    pics/nucleussmall/.style={code={%
        \pgfmathdeclarerandomlist{nucleon}{{proton}{proton}{neutron}{neutron}{neutron}}
        \pgfmathsetseed{#1+1}
        \foreach \A/\R in {8/0.2, 5/0.13, 1/0}{
        \pgfmathsetmacro{\S}{360/\A}
            \foreach \B in {0,\S,...,360}{
                \pgfmathrandomitem{\C}{nucleon}
                \pic at ($(\B+2*\A+5*rnd:\R)$) {\C}; } }} },
    pics/nucleusbig/.style={code={%
        \pgfmathdeclarerandomlist{nucleon}{{proton}{proton}{neutron}{neutron}{neutron}}
        \pgfmathsetseed{#1+1}
        \foreach \A/\R in {24/0.4, 24/0.3, 24/0.2, 13/0.35, 11/0.27, 6/0.15, 1/0}{
        \pgfmathsetmacro{\S}{360/\A}
            \foreach \B in {0,\S,...,360}{
                \pgfmathrandomitem{\C}{nucleon}
                \pic at ($(\B+2*\A+5*rnd:\R)$) {\C}; } }} },
    pics/nucleusbiggest/.style={code={%
        \pgfmathdeclarerandomlist{nucleon}{{proton}{proton}{neutron}{neutron}{neutron}}
        \pgfmathsetseed{#1+1}
        \foreach \A/\R in {24/0.5, 24/0.4, 24/0.3, 24/0.2, 13/0.47, 15/0.44, 13/0.37, 11/0.27, 6/0.15, 1/0}{
        \pgfmathsetmacro{\S}{360/\A}
            \foreach \B in {0,\S,...,360}{
                \pgfmathrandomitem{\C}{nucleon}
                \pic at ($(\B+2*\A+5*rnd:\R)$) {\C}; } }} },
    }
 \tikzset{square left brace/.style={ncbar=0.5cm}}
\tikzset{square right brace/.style={ncbar=-0.5cm}}
 \begin{scope}[shift={(0,0)},rotate=00] 
\filldraw[even odd rule,inner color=mydarkred,outer color=white, line width=0, draw=white] (0,0) circle (2.0);
  \coordinate (O)  at (0,0);
   \fill[radius=2.0pt,mydarkblue] (O) circle ;
\pic at (0,0) {nucleussmall} node at  (0,-1) { } ;

\draw[ -, >=latex'](0.3,0.2) -- coordinate (x axis) (0.9,0.9); 
\draw[ -, >=latex'](0.9,-0.9) -- coordinate (x axis) (1.7,-1.7);       
      
\draw[decorate,decoration={brace,amplitude=3pt,mirror}] (1.1,1.5) coordinate (t_k_unten) -- (1.1,0.5) coordinate (t_k_opt_unten); 
\node at (1.5,1.2){$n_p$=56};
\node at (1.5,0.7){$n_n$=81};
\node at (2.2,-1.7){$n_e$=54};
\draw [red, thick] (-2,-2) to [square left brace ] (-2,2);
\draw [red, thick] (2.2,-2) to [square right brace] (2.2,2);
\node [red] at (3,2){\Large $2+$};
  \end{scope}
\end{tikzpicture}\end{center}
 \end{question}
\begin{solution}
 \ce{^{137}_{56}Ba^{2+}}
\hspace{0.1cm}\end{solution}\stepcounter{numA}
