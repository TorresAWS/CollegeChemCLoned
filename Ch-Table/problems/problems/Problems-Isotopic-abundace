%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Write down the isotopic symbol given the number of protons, neutrons and electrons:
\begin{inparaenum}[(a)]
\item     43 protons, 43 electrons and 60 neutrons	%\ce{^{103}_{43}Pd}
\item      48 protons, 48 electrons and 65 neutrons	%\ce{^{113}_{48}Cd}
\item      77 protons, 77 electrons and 115 neutrons	%\ce{^{192}_{77}Ir}
   \end{inparaenum}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
 \item      \ce{^{103}_{43}Pd}
\item       \ce{^{113}_{48}Cd}
\item       \ce{^{192}_{77}Ir}
\end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}
