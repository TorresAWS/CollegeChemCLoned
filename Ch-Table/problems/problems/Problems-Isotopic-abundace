%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
The atomic mass of Ga is 69.72 amu. There are only two naturally occurring isotopes of gallium: 69Ga, with a mass of 69.0 amu, and 71Ga, with a mass of 71.0 amu. Calculate the natural abundance of the 69Ga isotope. 
\end{question}
\begin{solution}
64\%
\hspace{0.1cm}\end{solution}\stepcounter{numA}

%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
The atomic mass of Xe is 131.293 amu. There are only two naturally occurring isotopes of Xe: Xenon-133, with a mass of 133.0 amu, and Xenon-135, with a mass of 135 amu. Calculate the natural abundance of the Xenon-135 isotope. 
\end{question}
\begin{solution}
15\%
\hspace{0.1cm}\end{solution}\stepcounter{numA}


 
%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Magnesium contains three different isotopes: magnesium-24 with an abundance of 79\% and a mass of 23.9850423 amu, magnesium-25 with an abundance of 10\% and a mass of 24.9858374 amu, and magnesium-26 with a mass of 25.9825937 amu. Calculate the abundance of magnesium-26 and the average atomic mass of a sample of magnesium.
\end{question}
\begin{solution}
24.3048 amu
\hspace{0.1cm}\end{solution}\stepcounter{numA}

%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Silicon contains three different isotopes:  Si-28 with a mass 27.976927amu and abundace of 92.2297\%, Si-29 with a mass 28.976495amu and abundace of 4.6832\% and Si-30 with a mass 29.973770amu. Calculate the abundance of Si-30 and the average atomic mass of a sample of Si.
\end{question}
\begin{solution}
3.0872\%; 28.0854amu
\hspace{0.1cm}\end{solution}\stepcounter{numA}





%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
There are only two naturally occurring isotopes of Lutetium: Lu-175 with a mass of 174.94 amu and abundace of 97.41\% , and Lu-176 with a mass of 175.94 amu and abundace of 2.59\%. Calculate the average mass of the element using the table below, givem $m_i$ is the isotopic mass and $f_i$ is the fractional abundance of the isotope.
\begin{center}\newcolumntype{Y}{>{\raggedright\arraybackslash}X}\begin{tabularx}{\columnwidth}{YYYYY}
\toprule
 Isotope	&	$m_i$, 	&		$f_i$ & $m_i \times f_i$ &	 \\
  	&	  amu	&		  &   &	 \\
\midrule
    			&  			&	 		 &  & \\
     		&  			&	 		 &  	 &\\
\bottomrule
 %  \diag{.1em}{1.5cm}{ }{ } & \multicolumn{2}{c}{Average mass, amu=}	 &  	 \\
    \multicolumn{2}{l}{Average mass =}& &\multicolumn{2}{l}{$\sum  m_i\times f_i=$}   	    	 \\
    \multicolumn{2}{c}{   amu}& &\multicolumn{2}{l}{ }   	    	 \\
\bottomrule
\end{tabularx}\end{center}
\end{question}
\begin{solution}
174.965 amu
\hspace{0.1cm}\end{solution}\stepcounter{numA}

%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
There are only two naturally occurring isotopes of Thallium: Tl-203 with mass of 202.972 amu and abundace of  29.52\%, and Tl-205 with mass of 204.974 amu and abundance of  70.48\%. Calculate the average mass of the element using the table below, givem $m_i$ is the isotopic mass and $f_i$ is the fractional abundance of the isotope.
\begin{center}\newcolumntype{Y}{>{\raggedright\arraybackslash}X}\begin{tabularx}{\columnwidth}{YYYYY}
\toprule
 Isotope	&	$m_i$ 	&		$f_i$ & $m_i \times f_i$ &	 \\
  	&	  (amu)	&		  &   &	 \\
\midrule
     		&   			&  		 &   & \\
      		&   			& 	 		 &   	 &\\
\bottomrule
    \multicolumn{2}{l}{Average atomic mass }& &\multicolumn{2}{l}{$=\sum  m_i\times f_i=$  }   	    	 \\
    \multicolumn{2}{c}{  (amu)}& &\multicolumn{2}{l}{ }   	    	 \\
\bottomrule
\end{tabularx}\end{center}
\end{question}
\begin{solution}
146.790 amu
\hspace{0.1cm}\end{solution}\stepcounter{numA}



%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Fill the table below and calculate the missing variable.
\begin{center}\newcolumntype{Y}{>{\raggedright\arraybackslash}X}\begin{tabularx}{\columnwidth}{YYYYY}
\toprule
 Isotope	&	$m_i$ 	&		$f_i$ & $m_i \times f_i$ &	 \\
  	&	  (amu)	&		  &   &	 \\
\midrule
  \ce{^{107}Ag}  		&106.9050   			&$0.5184 $	 		 	&?  & \\
  \ce{^{109}Ag}   		&108.9047  				&$0.4816  $	 		 &?	  	 &\\
\bottomrule
    \multicolumn{2}{l}{Average atomic mass }& &\multicolumn{2}{l}{$=\sum  m_i\times f_i=$? }   	    	 \\
    \multicolumn{2}{c}{  (amu)}& &\multicolumn{2}{l}{ }   	    	 \\
\bottomrule
\end{tabularx}\end{center} 

\end{question}
\begin{solution}
107.8680 amu
\hspace{0.1cm}\end{solution}\stepcounter{numA}


%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Fill the table below and calculate the missing variable.
\begin{center}\newcolumntype{Y}{>{\raggedright\arraybackslash}X}\begin{tabularx}{\columnwidth}{YYYYY}
\toprule
 Isotope	&	$m_i$ 	&		$f_i$ & $m_i \times f_i$ &	 \\
  	&	  (amu)	&		  &   &	 \\
\midrule
  \ce{^{137}Ba}  		&	136.9058    			&?	 		 	&?  & \\
  \ce{^{138}Ba}   		&	137.9052   				&?	 		 	&?	  	 &\\\bottomrule
\bottomrule
    \multicolumn{2}{l}{Average atomic mass }& \multicolumn{3}{l}{$=\sum  m_i\times f_i=$114.38 }   	    	 \\
    \multicolumn{2}{c}{  (amu)}& &\multicolumn{2}{l}{ }   	    	 \\
\bottomrule
\end{tabularx}\end{center} 

\end{question}
