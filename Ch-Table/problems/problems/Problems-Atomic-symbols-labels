\begin{inparaenum}[(a)]
\item Sc %Scandium
\item Se %Selenium
\item Si %Silicon
\item Ag %Silver
\item Na %Sodium
\item Sr %Strontium
\item S %Sulfur
\item Te %Tellurium
\end{inparaenum}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
\item  Scandium
\item  Selenium
\item  Silicon
\item  Silver
\item  Sodium
\item  Strontium
\item  Sulfur
\item  Tellurium
\end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}
