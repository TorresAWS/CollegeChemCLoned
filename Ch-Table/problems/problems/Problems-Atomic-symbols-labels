%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Write down the symbol for the following elements:
\begin{inparaenum}[(a)]
 \item Magnesium %Mg
\item Manganese %Mn
\item Mercury %Hg
\item Molybdenum %Mo
\item Neodymium %Nd
\item Neon %Ne
\item Neptunium %Np
\item Nickel %Ni
\item Osmium %Os
\item Palladium %Pd
\end{inparaenum}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
 \item  Mg
\item  Mn
\item  Hg
\item  Mo
\item  Nd
\item  Ne
\item  Np
\item  Ni
\item  Os
\item  Pd
\end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}
%%%%%%%%%%%%%%
%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Write down the symbol for the following elements:
\begin{inparaenum}[(a)]
\item Phosphorus %P
\item Platinum %Pt
\item Plutonium %Pu
\item Polonium %Po
\item Potassium %K
\item Radium %Ra
\item Radon %Rn
\item Rhenium %Re
\item Rhodium %Rh
\item Rubidium %Rb
\item Ruthenium %Ru
\end{inparaenum}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
\item  P
\item  Pt
\item  Pu
\item  Po
\item  K
\item  Ra
\item  Rn
\item  Re
\item  Rh
\item  Rb
\item  Ru
\end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}
