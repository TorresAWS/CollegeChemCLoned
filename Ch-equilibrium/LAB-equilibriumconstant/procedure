     \section*{Calculations}
\mycircled{1} This is the volume of \ce{Fe(NO3)3} added.\vspace{0.5cm}\\
\mycircled{2} This is the volume of \ce{KSCN} added.\vspace{0.5cm}\\
\mycircled{3} This is the total volume of the mixtures.\vspace{0.5cm}\\
\mycircled{4} This is the initial Iron(III) concentration in the mixtures:\vspace{0.5cm}\\
    \[[\ce{Fe^{+3}}]_{0}= \frac{v_{\ce{Fe^{+3}}}\cdot c_{\ce{Fe^{+3}}}}{v_{total}}=\frac{  \;\;\mycircled{1}\;\; \cdot 0.25}{\;\;\mycircled{3}} \]
\mycircled{5} This is the initial thiocyanide concentration in the mixtures:\vspace{0.5cm}\\
    \[[\ce{SCN^{-}}]_{0}= \frac{v_{\ce{SCN^{-}}}\cdot c_{\ce{SCN^{-}}}}{v_{total}}=\frac{\;\;\mycircled{2}\;\; \cdot 10^{-4} }{\;\;\mycircled{3}\;\;} \]
\mycircled{6} This is the concentration of  \ce{Fe(SCN)^{+2}} in the mixture:\vspace{0.5cm}\\
    \[[\ce{Fe(SCN)^{+2}}]= [\ce{SCN^{-}}]=   \;\;\mycircled{5}\;\;   \]
\mycircled{7} This is the measured absorbance of the mixture.\vspace{0.5cm}\\
\mycircled{8} This step is needed to calculate numerically the Lambert-Beer's constant $k$:\vspace{0.5cm}\\
    \[[\ce{Fe(SCN)^{+2}}]\cdot A=   \;\;\mycircled{6}\;\;\cdot\;\;\mycircled{7}\;\;    \]
\mycircled{9} This step is needed to calculate numerically the Lambert-Beer's constant $k$:\vspace{0.5cm}\\
    \[[\ce{Fe(SCN)^{+2}}]^2=   \;\;\mycircled{6}\;\;^2    \]
\mycircled{10} This step is needed to calculate numerically Lambert-Beer's constant $k$, and results from adding all values of \;\;\mycircled{8}\;\;.\vspace{0.5cm}\\
\mycircled{11} This step is needed to calculate numerically Lambert-Beer's constant $k$, and results from adding all values of \;\;\mycircled{9}\;\;.\vspace{0.5cm}\\
 \mycircled{12} This step the Lambert-Beer's constant $k$:\vspace{0.5cm}\\
    \[k=\frac{\sum [\ce{Fe(SCN)^{+2}}]\cdot A}{\sum [\ce{Fe(SCN)^{+2}}]^2} =  \frac{\;\;\mycircled{10}\;\;}{\;\;\mycircled{11}\;\;}    \]
 \mycircled{13} This is the volume of \ce{Fe(NO3)3} added.\vspace{0.5cm}\\
\mycircled{14} This is the volume of \ce{KSCN} added.\vspace{0.5cm}\\
\mycircled{15} This is the total volume of the mixtures.\vspace{0.5cm}\\
\mycircled{16} This is the initial Iron(III) concentration in the mixtures:\vspace{0.5cm}\\
    \[[\ce{Fe^{+3}}]_{0}= \frac{v_{\ce{Fe^{+3}}}\cdot c_{\ce{Fe^{+3}}}}{v_{total}}=\frac{  \;\;\mycircled{13}\;\; \cdot 0.0025}{\;\;\mycircled{15}} \]
\mycircled{17} This is the initial thiocyanide concentration in the mixtures:\vspace{0.5cm}\\
    \[[\ce{SCN^{-}}]_{0}= \frac{v_{\ce{SCN^{-}}}\cdot c_{\ce{SCN^{-}}}}{v_{total}}=\frac{\;\;\mycircled{14}\;\; \cdot  0.0025 }{\;\;\mycircled{15}\;\;} \]
\mycircled{18} This is the measured absorbance of the mixture.\vspace{0.5cm}\\
\mycircled{19} This is the concentration of \ce{Fe(SCN)^{+2}} in equilibrium:\vspace{0.5cm}\\
    \[[\ce{Fe(SCN)^{+2}}]_{eq}= \frac{A}{k}=  \frac{\;\;\mycircled{18}\;\;}{\;\;\mycircled{12}\;\;}    \]
\mycircled{20} This is the concentration of \ce{Fe^{+3}} in equilibrium:\vspace{0.5cm}\\
    \[[\ce{Fe^{+3}}]_{eq}= [\ce{Fe^{+3}}]_{0}-[\ce{Fe(SCN)^{+2}}]_{eq}=   \;\;\mycircled{16}\;\; - \;\;\mycircled{19}\;\;     \]
\mycircled{21} This is the concentration of \ce{SCN^{-}} in equilibrium:\vspace{0.5cm}\\
    \[[\ce{SCN^{-}}]_{eq}= [\ce{SCN^{-}}]_{0}-[\ce{Fe(SCN)^{+2}}]_{eq}=   \;\;\mycircled{17}\;\; - \;\;\mycircled{19}\;\;     \]
\mycircled{22} This is the equilibrium constant:\vspace{0.5cm}\\
    \[K= \frac{[\ce{Fe(SCN)^{+2}}]_{eq}}{[\ce{Fe^{+3}}]_{eq}\cdot [\ce{SCN^{-}}]_{eq}}= \frac{\;\;\mycircled{19}\;\;}{\;\;\mycircled{20}\;\;\cdot \;\;\mycircled{21}\;\;}        \]














