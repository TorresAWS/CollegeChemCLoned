\begin{tikzpicture} 
\begin{scope}[shift={(0em,0)}	,scale=0.8]
 \draw [->, black, shift={(0,0)}]  (0,0) -- (5,0) node [rotate=0, shift={(-6em,-1em)}]{Volume, (mL)} ;
 \draw [->, black, shift={(0,0)}]  (0,0) -- (0,5) node [rotate=90, shift={(-5em,2em)}]{PH} ;
\draw [red, thick, shift={(-8em,2em)}] plot [smooth, tension=0.35] coordinates { (2.8,0.8) (3,1.0) (4.7,1.5) (5.1,3.0) (6.5, 3.5)} node[shift={(-6em,-4em)}] {\tiny $c$} ;
\draw [cyan, thick, shift={(-8em,2em)}] plot [smooth, tension=0.35] coordinates { (2.8,0.3) (3,0.5) (4.7,0.9) (5.1,3.0) (6.5, 3.5)} node[shift={(-6em,-6em)}] {\tiny $b$};
