
%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
We titrate 10mL of a 0.1M-HCl solution with 0.05M-NaOH using an indicator with $K_{In}=1\times 10^{-7}$ that is blue on its acid form and red on its basic form. Answer the following questions:
\begin{inparaenum}[(a)]
\item  What is the initial color of the solution  	%blue
\item  After adding 20mL of NaOH, indicate the color of the solution  	%green
\item  After adding 30mL of NaOH, indicate the color of the solution  	%red
\item  What is the PH of the equivalency point  	%PH=7
\item  At what PH will the color transition occur using the 10\% rule  	%PH=6 using the 10\% rule.
 \end{inparaenum}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
\item   blue
\item   green
\item   red
\item   PH=7
\item   PH=6 using the 10\% rule.
 \end{inparaenum}
 \hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%%


 
%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
We titrate 10mL a 0.1M-\ce{CH3COOH} ($K_a=1.8\times 10^{-5}$) solution with 0.01M-NaOH using methyl red ($K_{In}=9\times 10^{-6}$) that is yellow on its acid form and orange on its basic form. Answer the following questions:
\begin{inparaenum}[(a)]
\item  What is the initial color of the solution  	%yellow
\item  After adding 50mL of NaOH, indicate the color of the solution  	%yellow
\item  After adding 150mL of NaOH, indicate the color of the solution  	%orange
\item  What is the PH of the equivalency point  
\item  At what PH will the color transition occur using the 10\% rule  	%PH=4 using the 10\% rule.
 \end{inparaenum}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
\item   yellow
\item   yellow
\item   orange
\item   PH=8.35
\item   PH=4-6 using the 10\% rule.
 \end{inparaenum}
 \hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%%



%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Which of the following indicators can be used in the titration of a 20mL sample of 0.01M-\ce{NH3} with 0.1M-\ce{HCl}:
\begin{inparaenum}[(a)]
\item  Thymol blue  	($pK_{In}=8.9$)
\item  Phenolphthalein  	($pK_{In}=9.4$)
\item  Bromophenol blue ($pK_{In}=3.8$)
\item  Cresol red ($pK_{In}=1.0$)
 \end{inparaenum}
\end{question}
\begin{solution}
Bromophenol blue
 \hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%%


%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Which of the following indicators can be used in the titration of a 20mL sample of 0.1M-\ce{CH3COOH} ($K_a=.18\times 10^{-5}$) with 0.1M-\ce{KOH}:
\begin{inparaenum}[(a)]
\item  Thymol blue  	($pK_{In}=8.9$)
\item  Phenolphthalein  ($pK_{In}=9.4$)
\item  Bromophenol blue ($pK_{In}=3.8$)
\item  Cresol red ($pK_{In}=1.0$)
 \end{inparaenum}
\end{question}
\begin{solution}
Thymol blue (PH at equivalncy is 10.96) 
 \hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%%


%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
An indicator with $pK_{In}=5$ has a color transition that becomes noticeable when 3\% of its acid form has been comforted into a base. Indicate the PH at which the color transition starts to happen.
\end{question}
\begin{solution}
3.5 
 \hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%%



%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
A solution is buffered to PH=3. Indicate the color of the following indicators:
\begin{inparaenum}[(a)]
\item  Thymol blue  	($pK_{In}=8.9$, Y-B) %yellow
\item  Phenolphthalein  ($pK_{In}=9.4$, Colorless-R)%colorless
\item  Bromophenol blue ($pK_{In}=3.8$, Y-B)%blue
\item  Cresol red ($pK_{In}=1.0$, R-Y)%yellow
 \end{inparaenum}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
\item   yellow
\item   colorless
\item   blue
\item   yellow
 \end{inparaenum}
 \hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%%



