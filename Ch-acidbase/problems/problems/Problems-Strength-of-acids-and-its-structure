%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Compare the acidity of the following pairs and justify the mechanism responsible for your choice:
\begin{inparaenum}[(a)]
\item \ce{H2O} and \ce{H2S} % (\ce{H2S} due to MW of central atom)
\item \ce{SiH4} and \ce{PH3} % (\ce{PH3} due to electronegativity of central atom)
\item \ce{CH3COOH} and \ce{CH2FCOOH} % (\ce{CH2FCOOH} due indirect electronegative atom)
\item \ce{CH2ClCOOH} and \ce{CH2FCOOH} % (\ce{CH2FCOOH} due indirect electronegative atom)
 \end{inparaenum}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
 \item  \ce{H2S} due to MW of central atom
\item  \ce{PH3} due to electronegativity of central atom
\item  \ce{CH2FCOOH} due indirect electronegative atom
\item  \ce{CH2FCOOH} due indirect electronegative atom
 \end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%%

%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Compare the acidity of the following pairs and justify the mechanism responsible for your choice:
\begin{inparaenum}[(a)]
\item \ce{H2S} and \ce{HCl} % (\ce{HCl} due to electronegativity of central atom)
\item \ce{SiH4} and \ce{GeH4} % (\ce{GeH4} due to MW of central atom)
\item \ce{CH3CH2OH} and \ce{CH3COOH} % (\ce{CH3COOH} due to larger number of oxygens)
\item \ce{CH3CH2OH} and \ce{CH3CHFOH} % (\ce{CH3CHFOH} due to EN)
 \end{inparaenum}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
\item  \ce{HCl} due to electronegativity of central atom 
\item  \ce{GeH4} due to MW of central atom 
\item  \ce{CH3COOH} due to larger number of oxygens 
\item  \ce{CH3CHFOH} due to EN 
 \end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%%
