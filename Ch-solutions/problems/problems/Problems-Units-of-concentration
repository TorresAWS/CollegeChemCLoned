\item  a 1M \ce{H2CO3} solution  % 2N
\item  a 2M \ce{H3PO4} solution  % 6N
\end{inparaenum}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
\item    2N
\item    2N
\item    6N
 \end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%%

 


