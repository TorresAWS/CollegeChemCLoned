%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Calculate normality for the following cases:
\begin{inparaenum}[(a)]
\item  a 1M \ce{KMnO4} solution given it provides 5 $e^-$ in a redox  % 5N
\item  a 2M \ce{K2Cr2O7} solution given it provides 6 $e^-$ in a redox  % 12N
\item  a 3M \ce{KOH} solution  % 3N
\item  a 3M \ce{Ca(OH)_2} solution  % 6N
\end{inparaenum}
\end{question}
\begin{solution}
\begin{inparaenum}[(a)]
\item    5N
\item    12N
\item    3N
\item    6N
 \end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}%%%%%%%%%%%%



%%%%%%PROBLEM
