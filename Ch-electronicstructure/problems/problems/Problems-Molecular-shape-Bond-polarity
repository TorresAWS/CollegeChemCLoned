  \draw[line width=2pt,blue,-stealth](0,0)--(30:6em) node[anchor=south west]{$\boldsymbol{\mu_1}$};
  \draw[line width=2pt,red,-stealth](0,0)--(150:6em) node[anchor=north east]{$\boldsymbol{\mu_2}$};
  \draw[line width=2pt,red,-stealth](0,0)--(-90:6em) node[anchor=north east]{$\boldsymbol{\mu_3}$};
\end{tikzpicture}\end{center}
\end{question}
\begin{solution} 
not overall dipole
\hspace{0.1cm}\end{solution}
