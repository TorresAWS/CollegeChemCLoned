%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Select the more polar bond from the following set of bond pairs:
\begin{inparaenum}[(a)]
      \item \ce{N\bond{-}O} or  \ce{N\bond{-}P}  %\ce{N\bond{-}O}
      \item \ce{P\bond{-}Si} or  \ce{P\bond{-}S}  %\ce{P\bond{-}S}
      \item \ce{S\bond{-}Cl} or  \ce{S\bond{-}Se}  %\ce{S\bond{-}Cl} 
\end{inparaenum}
\end{question}
\begin{solution} 
 \begin{inparaenum}[(a)]
      \item \ce{N\bond{-}O}
      \item  \ce{P\bond{-}S}
      \item  \ce{S\bond{-}Cl} 
 \end{inparaenum}
\hspace{0.1cm}\end{solution}
\stepcounter{numA}
%%%%%%%%%%%%%%
 
 %%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Select the more polar bond from the following set of bond pairs:
\begin{inparaenum}[(a)]
      \item \ce{H\bond{-}O} or  \ce{H\bond{-}F}  % \ce{H\bond{-}F} 
      \item \ce{O\bond{-}S} or  \ce{S\bond{-}Se}  %\ce{O\bond{-}S}
     \item \ce{H\bond{-}Cl} or  \ce{H\bond{-}F}  % \ce{H\bond{-}F}
\end{inparaenum}
\end{question}
\begin{solution} 
 \begin{inparaenum}[(a)]
      \item   \ce{H\bond{-}F} 
      \item  \ce{O\bond{-}S}
     \item   \ce{H\bond{-}F}
 \end{inparaenum}
\hspace{0.1cm}\end{solution}
