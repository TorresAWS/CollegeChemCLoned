%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Indicate the polar character (polar, nonpolar) of the following molecules:
\begin{inparaenum}[(a)]
\item \ce{H2O}	%(Polar)
\item  \ce{HCl}	%(Polar)
\item  \ce{CO2}	%(non Polar)
\item  \ce{CO}	%(Polar)
\item  \ce{PF3}	%(Polar)
\item  \ce{SiCl4}%(non Polar)
\end{inparaenum}
\end{question}
\begin{solution} 
 \begin{inparaenum}[(a)]
 \item   Polar 
\item   Polar 
\item   non Polar 
\item  Polar 
\item   Polar 
\item   non Polar 
 \end{inparaenum}
\hspace{0.1cm}\end{solution}
\stepcounter{numA}
%%%%%%%%%%%%%%


%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Indicate the polar character (polar, nonpolar) of the following molecules:
\begin{inparaenum}[(a)]
\item  \ce{CF4}	%(non Polar)
\item  \ce{NH3}	%(Polar)
\item \ce{SeF2}	%(Polar)
\item  \ce{O2}%(non Polar)
\item  \ce{CH4}	%(non Polar)
\item  \ce{H2}	%(non Polar)
\end{inparaenum}
\end{question}
\begin{solution} 
 \begin{inparaenum}[(a)]
\item   non Polar 
\item   Polar 
\item  Polar 
\item   non Polar 
\item   non Polar 
\item   non Polar 
 \end{inparaenum}
\hspace{0.1cm}\end{solution}
