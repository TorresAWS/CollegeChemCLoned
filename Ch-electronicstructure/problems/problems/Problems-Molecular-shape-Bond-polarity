  %%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Predict if the following dipole distribution will lead to an overall dipole moment:
 \begin{center}\begin{tikzpicture}
  \draw[thin,gray!40] (-2,-2) grid (2,2);
  \draw[<->] (-2,0)--(2,0) node[right]{$x$};
  \draw[<->] (0,-2)--(0,2) node[above]{$y$};
  \draw[line width=2pt,blue,-stealth](0,0)--(45:6em) node[anchor=south west]{$\boldsymbol{\mu_1}$};
  \draw[line width=2pt,red,-stealth](0,0)--(-135:6em) node[anchor=north east]{$\boldsymbol{\mu_2}$};
\end{tikzpicture}\end{center}
\end{question}
\begin{solution} 
no overall dipole
\hspace{0.1cm}\end{solution}
\stepcounter{numA}
%%%%%%%%%%%%%%
 
   %%%%%%%PROBLEM
