
%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Describe the trend in electronegativity (increases, decreases):
\begin{inparaenum}[(a)]
 \item from Na to Mg	%increases
 \item from Cs to Ba	%increases
\item from S to O		%increases
\item from P to C		%increases
\item from K to Rb		%decreases
\item from B to C		%increases
\item from Li to Na		%decreases
\end{inparaenum}
\end{question}
\begin{solution} 
 \begin{inparaenum}[(a)]
  \item from Na to Mg	% increases 
 \item from Cs to Ba	% increases 
\item from S to O		% increases
\item from P to C		% increases
\item from K to Rb		% decreases
\item from B to C		% increases
\item from Li to Na		% decreases
 \end{inparaenum}
\hspace{0.1cm}\end{solution}
\stepcounter{numA}
%%%%%%%%%%%%%%
%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Describe the trend in electronegativity (increases, decreases):
\begin{inparaenum}[(a)]
 \item from S to Cl		%increases
\item from Se to Br		%increases
\item from Rb to Cs		%decreases
\item from P to O		%increases
\item from Cl to F		%increases
\item from Li to Be		%increases
\item from Se to Cl		%increases
\end{inparaenum}
\end{question}
\begin{solution} 
 \begin{inparaenum}[(a)]
 \item from S to Cl		% increases
\item from Se to Br		% increases
\item from Rb to Cs		% decreases
\item from P to O		% increases
\item from Cl to F		% increases
\item from Li to Be		% increases
\item from Se to Cl		% increases
 \end{inparaenum}
\hspace{0.1cm}\end{solution}
\stepcounter{numA}
%%%%%%%%%%%%%%


%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Arrange the atoms in the following set in order of increasing electronegativity:
\begin{inparaenum}[(a)]
 \item Li, Be, and B	%Li$<$Be$<$B
 \item  Rb,	Na, and  K	%Rb$<$K$<$Na
 \item  N, C,  and B 	%B$<$C$<$N
\end{inparaenum}
\end{question}
\begin{solution} 
 \begin{inparaenum}[(a)]
  \item  Li$<$Be$<$B
 \item   Rb$<$K$<$Na
 \item   B$<$C$<$N
 \end{inparaenum}
\hspace{0.1cm}\end{solution}
\stepcounter{numA}
%%%%%%%%%%%%%%

%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Arrange the atoms in the following set in order of increasing electronegativity:
\begin{inparaenum}[(a)]
 \item  P, Cl, and S 	%P$<$S$<$Cl
 \item  Br, F, and  Cl	%Br$<$Cl$<$F
 \item  S, O,  and  Se	%Se$<$S$<$O
\end{inparaenum}
\end{question}
\begin{solution} 
 \begin{inparaenum}[(a)]
 \item   P$<$S$<$Cl
 \item   Br$<$Cl$<$F
 \item   Se$<$S$<$O
 \end{inparaenum}
\hspace{0.1cm}\end{solution}
\stepcounter{numA}
