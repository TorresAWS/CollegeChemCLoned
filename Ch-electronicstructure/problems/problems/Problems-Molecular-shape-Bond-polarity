%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Classify the following bonds as ionic, polar colavent and nonpolar covalent by calculating the electronegative difference:
\begin{inparaenum}[(a)]
 \item  \ce{S\bond{-}P} 	%nonpolar covalent ($\Delta$EN=0.4)
 \item  \ce{O\bond{-}Mg} 	%ionic ($\Delta$EN=2.1)
 \item  \ce{H\bond{-}Na} 	%polar covalent ($\Delta$EN=1.3)
 \item  \ce{H\bond{-}N} 	%polar covalent ($\Delta$EN=0.8)
 \item  \ce{Na\bond{-}Cl} 	%ionic ($\Delta$EN=2.2)
 \item  \ce{S\bond{-}C} 	%nonpolar covalent ($\Delta$EN=0.0)
\end{inparaenum}
\end{question}
\begin{solution} 
 \begin{inparaenum}[(a)]
  \item   nonpolar covalent ($\Delta$EN=0.4)
 \item   ionic ($\Delta$EN=2.1)
 \item   polar covalent ($\Delta$EN=1.3)
 \item   polar covalent ($\Delta$EN=0.8)
 \item   ionic ($\Delta$EN=2.2)
 \item   nonpolar covalent ($\Delta$EN=0.0)
 \end{inparaenum}
\hspace{0.1cm}\end{solution}
\stepcounter{numA}
%%%%%%%%%%%%%%

%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Classify the following bonds as ionic, polar colavent and nonpolar covalent by calculating the electronegative difference:
\begin{inparaenum}[(a)]
  \item  \ce{H\bond{-}K} 	%polar covalent ($\Delta$EN=1.4)
 \item  \ce{H\bond{-}Li} 	%polar covalent ($\Delta$EN=1.2)
 \item  \ce{O\bond{-}Li} 	%ionic ($\Delta$EN=2.5)
 \item  \ce{O\bond{-}Na} 	%ionic ($\Delta$EN=2.5)
 \item  \ce{Na\bond{-}F} 	%ionic ($\Delta$EN=3.0)
 \item  \ce{S\bond{-}H} 	%nonpolar covalent ($\Delta$EN=0.4)
\end{inparaenum}
\end{question}
\begin{solution} 
 \begin{inparaenum}[(a)]
  \item   polar covalent ($\Delta$EN=1.4)
 \item   polar covalent ($\Delta$EN=1.2)
 \item   ionic ($\Delta$EN=2.5)
 \item   ionic ($\Delta$EN=2.5)
 \item   ionic ($\Delta$EN=3.0)
 \item   nonpolar covalent ($\Delta$EN=0.4)
 \end{inparaenum}
\hspace{0.1cm}\end{solution}
\stepcounter{numA}
