\begin{inparaenum}[(a)]
 \item  \ce{I3^-}	  	%\hspace{.05in} \schemestart \chemleft[\hspace{.4cm}\chemfig{\chemabove{\lewis{136,I}}{\hspace{.1cm} \ominus }	( (-[:0]\lewis{206,I})(-[:180]\lewis{246,I}) )}\hspace{.4cm}\chemright] \chemmove{\node[] at (0pt,5pt) {\footnotesize $-$};}\schemestop  \hspace{.05in}
