%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Identify the name of the following molecular structure:
\usetikzlibrary{quotes,angles}
\newcommand{\Bond}[6]%
% start, end, thickness, incolor, outcolor, iterations
{ \begin{pgfonlayer}{background}
        \colorlet{InColor}{#4}
        \colorlet{OutColor}{#5}
        \foreach \I in {#6,...,1}
        {   \pgfmathsetlengthmacro{\r}{#3/#6*\I}
            \pgfmathsetmacro{\C}{sqrt(1-\r*\r/#3/#3)*100}
            \draw[InColor!\C!OutColor, line width=\r] (#1.center) -- (#2.center);
        }
    \end{pgfonlayer}
}\newcommand{\SingleBond}[2]%
{   \Bond{#1}{#2}{1mm}{white}{gray!50}{10}
}
\begin{center} \begin{tikzpicture}
[   
    hydrogen/.style={circle, ball color=white, minimum size=2.5mm, inner sep=0},
         emptyatom/.style={circle, ball color=orange!75, minimum size=5.0mm, inner sep=0}
]
   \setOrbitalDrawing{{very thin,  shading=ball, draw=none}}
 \begin{scope}[shift={(0em,0em)}  ] 
%%%SF6
\coordinate (S) at (0,0);
\coordinate (Cl1) at (0,1.5);
\coordinate (Cl2) at (1.15,0.51);\coordinate (Cl3) at (1.00,-0.68);
\coordinate (Cl4) at (0,-1.5);
\coordinate (Cl5) at (-1.1,-0.7);\coordinate (Cl6) at (-0.73,0.47);
\coordinate (Cl2b) at (0.95,0.55);\coordinate (Cl1b) at (0.4,2.5);
\coordinate (Cl5b) at (-1.8,-0.7);\coordinate (Cl6b) at (-0.63,0.47);
  \node[emptyatom]  at (S) { };\node[hydrogen]   at (Cl1) { };\node[hydrogen]  at (Cl2) { };\node[hydrogen]  at (Cl3) { };\node[hydrogen]  at (Cl4) { };\node[hydrogen]  at (Cl5) { };\node[hydrogen]  at (Cl6) { };
\SingleBond{S}{Cl1}\SingleBond{S}{Cl2}\SingleBond{S}{Cl3}\SingleBond{S}{Cl4}\SingleBond{S}{Cl5} \SingleBond{S}{Cl6} 
 %    \pic["90$^{\circ}$",draw=orange,<->,angle eccentricity=1.3,angle radius=1.5cm,densely dotted] {angle=Cl2b--S--Cl1b}; 
 % \pic["90$^{\circ}$",draw=orange,<->,angle eccentricity=1.2,angle radius=1.2cm, densely dotted] {angle=Cl6b--S--Cl5b};
 
 \end{scope}
\end{tikzpicture} \end{center}


\end{question}
\begin{solution} 
 Octahedral
\hspace{0.1cm}\end{solution}
\stepcounter{numA}
%%%%%%%%%%%%%%



%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Identify the name of the following molecular structure:
\usetikzlibrary{quotes,angles}
\newcommand{\Bond}[6]%
% start, end, thickness, incolor, outcolor, iterations
{ \begin{pgfonlayer}{background}
        \colorlet{InColor}{#4}
        \colorlet{OutColor}{#5}
        \foreach \I in {#6,...,1}
        {   \pgfmathsetlengthmacro{\r}{#3/#6*\I}
            \pgfmathsetmacro{\C}{sqrt(1-\r*\r/#3/#3)*100}
            \draw[InColor!\C!OutColor, line width=\r] (#1.center) -- (#2.center);
        }
    \end{pgfonlayer}
}\newcommand{\SingleBond}[2]%
{   \Bond{#1}{#2}{1mm}{white}{gray!50}{10}
}
\begin{center} \begin{tikzpicture}
[   
    hydrogen/.style={circle, ball color=white, minimum size=2.5mm, inner sep=0},
         emptyatom/.style={circle, ball color=orange!75, minimum size=5.0mm, inner sep=0}
]
   \setOrbitalDrawing{{very thin,  shading=ball, draw=none}}
 \begin{scope}[shift={(0em,0em)}  ] 
\coordinate (S) at (0,0);
\coordinate (Cl1) at (0,1);
\coordinate (Cl2) at (1,0);\coordinate (Cl3) at (0,-1);
\coordinate (Cl4) at (-1,0);
\coordinate (Cl5) at (-1.1,-0.7);\coordinate (Cl6) at (-0.73,0.47);
\coordinate (Cl2b) at (0.95,0.55);\coordinate (Cl1b) at (0.4,2.5);
\coordinate (Cl5b) at (-1.8,-0.7);\coordinate (Cl6b) at (-0.63,0.47);
  \node[emptyatom]  at (S) { };\node[hydrogen]   at (Cl1) { };\node[hydrogen]  at (Cl2) { };\node[hydrogen]  at (Cl3) { };\node[hydrogen]  at (Cl4) { }; 
\SingleBond{S}{Cl1}\SingleBond{S}{Cl2}\SingleBond{S}{Cl3}\SingleBond{S}{Cl4}  
 \end{scope}
\end{tikzpicture} \end{center}
\end{question}
\begin{solution} 
square planar
\hspace{0.1cm}\end{solution}
\stepcounter{numA}
