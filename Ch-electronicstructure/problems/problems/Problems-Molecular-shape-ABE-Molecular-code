%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Identify the molecular shape of the molecules:
\begin{inparaenum}[(a)]
\item \ce{NH3} %Trigonal pyramidal
\item \ce{CH4} %Tetrahedral
\end{inparaenum}
\end{question}
\begin{solution} 
 \begin{inparaenum}[(a)]
\item \ce{NH3}  (Trigonal pyramidal)
\item \ce{CH4}  (Tetrahedral)
 \end{inparaenum}
\hspace{0.1cm}\end{solution}
\stepcounter{numA}
%%%%%%%%%%%%%%
%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Identify the molecular shape of the molecules:
\begin{inparaenum}[(a)]
\item \ce{H2}	%Linear
\item  \ce{BeCl2}	%Linear
 \item  \ce{BF3} %Trigonal planar
\end{inparaenum}
\end{question}
\begin{solution} 
 \begin{inparaenum}[(a)]
\item \ce{H2}	 (Linear)
\item  \ce{BeCl2}	(Linear)
 \item  \ce{BF3}  (Trigonal planar)
 \end{inparaenum}
\hspace{0.1cm}\end{solution}
\stepcounter{numA}
%%%%%%%%%%%%%%



%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Given the following Lewis structures, predict the molecular geometry and angles:
\vspace{0.5cm}\begin{multicols*}{2}
\begin{enumerate}[label=(\alph*)]
  \item  \hspace{.05in}\chemfig{\lewis{26,Se}( (-[:45]\lewis{713,Cl})(-[:135]\lewis{513,Cl}) )}\hspace{.05in}	% \ce{AB2E2}; bent; 109$^{\circ}$
 \item   \hspace{.05in}\chemfig{C( (=[:90]\lewis{13,O})(-[:210]H)(-[:330]H))}\hspace{.05in}   % \ce{ABE3}; planar trigonal; 120$^{\circ}$
 \end{enumerate}
\end{multicols*}
\end{question}
\begin{solution} 
 \begin{inparaenum}[(a)]
  \item    \ce{AB2E2}; bent; 109$^{\circ}$
 \item    \ce{AB2E2}; bent; 109$^{\circ}$ \ce{ABE3}; planar trigonal; 120$^{\circ}$
  \end{inparaenum}
\hspace{0.1cm}\end{solution}
\stepcounter{numA}
%%%%%%%%%%%%%%
%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Given the following Lewis structures, predict the molecular geometry and angles:
\vspace{0.5cm}\begin{multicols*}{2}
\begin{enumerate}[label=(\alph*)]
     \item  \hspace{.05in}\chemfig{B( (-[:0]H)(-[:180]H)(-[:270]\lewis{460,F}))}\hspace{.05in}% \ce{ABE3}; planar trigonal; 120$^{\circ}$ 
  \item   \hspace{.05in}\chemfig{P( (=[:90]\lewis{13,O})  (-[:210]\lewis{460,Cl})(-[:270]\lewis{460,Cl})(-[:320]\lewis{062,Cl})	)}\hspace{.05in} %\ce{AB4}; tetrahedral; 109.5$^{\circ}$  
 \end{enumerate}
\end{multicols*}
\end{question}
\begin{solution} 
 \begin{inparaenum}[(a)]
    \item   \ce{ABE3}; planar trigonal; 120$^{\circ}$ 
  \item   \ce{AB4}; tetrahedral; 109.5$^{\circ}$    
\end{inparaenum}
\hspace{0.1cm}\end{solution}
\stepcounter{numA}
%%%%%%%%%%%%%%

%%%%%%%PROBLEM
\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Given the following Lewis structures, predict the molecular geometry and angles:
\vspace{0.5cm}\begin{multicols*}{2}
\begin{enumerate}[label=(\alph*)]
   \item   \hspace{.05in}\chemfig{Cl( (=[:0]\lewis{17,O})  (=[:90]\lewis{13,O})(-[:180]\lewis{246,O})(=[:270]\lewis{57,O})	)}\hspace{.05in}	%\ce{AB4}; tetrahedral; 109.5$^{\circ}$
  \item   \hspace{.05in}\chemfig{Be( (-[:45]H)(-[:135]H) )}\hspace{.05in} %\ce{AB2}; linear; 180$^{\circ}$
   \end{enumerate}
\end{multicols*}
\end{question}
\begin{solution} 
 \begin{inparaenum}[(a)]
  \item   \ce{AB4}; tetrahedral; 109.5$^{\circ}$
  \item   \ce{AB2}; linear; 180$^{\circ}$
  \end{inparaenum}
\hspace{0.1cm}\end{solution}
\stepcounter{numA}
