%%%%%%%PROBLEM
\vspace{-0.0cm}\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Indicate the number of bonds that can the following atoms form:
 \begin{inparaenum}[(a)]
  \item    An atom with electron configuration ns$^2$np$^4$	 % 2 
  \item    C	  % 4 
  \item    An atom with electron configuration ns$^2$np$^2$ 	% 4 
  \item    An atom with electron configuration ns$^2$np$^1$	     % 3 
  \item    N	 % 3 
\item    H	% 1 
\end{inparaenum}
 \end{question}
\begin{solution}
 \begin{inparaenum}[(a)]
  \item      2 
  \item      4 
  \item      4 
  \item      3 
  \item      3 
\item      1 
\end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}


%%%%%%%PROBLEM
\vspace{-0.0cm}\begin{question}[ID=\the\value{numA}]\SetQuestionProperties{section-title=\nameref{sec:units}}
Indicate the number of bonds that can the following atoms form:
 \begin{inparaenum}[(a)]
   \item    An atom with electron configuration ns$^1$		% 1 
  \item    An atom with electron configuration ns$^2$np$^3$		% 4 
  \item    O		% 3 
  \item    An atom with electron configuration ns$^2$np$^5$		% 1 
  \item    P		% 3 
  \item    B		% 3 
\end{inparaenum}
 \end{question}
\begin{solution}
 \begin{inparaenum}[(a)]
  \item      1 
  \item      4 
  \item      3 
  \item      1 
  \item      3 
  \item      3  
\end{inparaenum}
\hspace{0.1cm}\end{solution}\stepcounter{numA}
